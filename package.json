        "minimist": "^1.2.5",
        "moxios": "^0.4.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "redux-mock-store": "^1.5.4",
        "sinon": "^9.2.4",
        "ts-jest": "^26.5.3",
        "typescript": "^4.2.2"
    },
    "jest": {
        "roots": [
            "<rootDir>/src/"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx"
        ],
        "moduleDirectories": [
            "../node_modules"
        ],
        "testPathIgnorePatterns": [
            ".*__mocks__.*",
            "<rootDir>/build"
        ],
        "moduleNameMapper": {
            "^components(.*)$": "<rootDir>/src/frontend/components$1",
            "^containers(.*)$": "<rootDir>/src/frontend/containers$1",
            "^actions(.*)$": "<rootDir>/src/frontend/store/actions$1",
            "^reducers(.*)$": "<rootDir>/src/frontend/store/reducers$1",
            "^middlewares(.*)$": "<rootDir>/src/frontend/store/middlewares$1",
            "^helpers(.*)$": "<rootDir>/src/frontend/helpers$1",
            "^frontendRoot(.*)$": "<rootDir>/src/frontend$1",
            "^interfaces(.*)$": "<rootDir>/src/frontend/interfaces$1"
        }
    },
    "lint-staged": {
        "*.{js, jsx, ts, tsx}": [
            "prettier --write",
            "git add"
        ]
    }
