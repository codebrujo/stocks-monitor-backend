{
    "name": "sergey-ogarkov-internship",
    "version": "1.0.0",
    "description": "Internship project of Sergey Ogarkov",
    "main": "index.js",
    "scripts": {
        "build": "npm run tsc && webpack --mode production",
        "dev": "webpack serve --hot --config webpack.dev.config.js --mode development",
        "devc": "concurrently \"tsc -w\" \"webpack serve --hot --config webpack.dev.config.js --mode development\"",
        "migrate": "sequelize-cli db:migrate",
        "devsrv": "npm run migrate && nodemon --watch src/backend ./src/backend/server.js",
        "test": "jest --coverage",
        "testw": "jest --watchAll",
        "tscw": "tsc -w",
        "tsc": "tsc",
        "precommit": "lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.dataart.com/asaltykov/sergey-ogarkov-internship.git"
    },
    "author": "Sergey Ogarkov",
    "license": "ISC",
